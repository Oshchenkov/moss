<script>
    $(document).ready(function() {
        $("#header@{@class_ext} .login").on('click', function () {
            window.location.href = "/login";
        });
        $("#header@{@class_ext} .blueLogo").on('click', function () {
            window.location.href = "http://employeeconfidential.com/";
        });

        $("#header@{@class_ext} .bigButton").on('click', function () {
            window.location.href = "/Report/new?";
        });
    });


</script>
