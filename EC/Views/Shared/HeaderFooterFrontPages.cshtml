<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <title>@ViewBag.Title</title>
    @*<link href="~/Content/fonts.css" rel="stylesheet" type="text/css">*@
    
    <link rel="stylesheet" type="text/css" href="@ViewBag.cssStyle">
