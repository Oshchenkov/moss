@using EC.App_LocalResources
@using EC.Controllers.Utils
@using EC.Models
@using EC.Localization;
@{
    ViewBag.Title = GlobalRes.increportingandcasemanag;
    Layout = "~/Views/Shared/_MainHTML.cshtml";
    bool is_cc = ViewBag.is_cc;
}
<div id="topBody">
    <div class="firstTitle">
        @Resources.GetString("FileReport")
    </div>
    <div class="secondTitle">
        @Resources.GetString("FindOrganization", is_cc)
    </div>
    <div class="thirdTitle">
        <input id="specialInput" placeholder="Company name or code">
        <div id="buttonStart"><span class="buttonStartText">@Resources.GetString("Start").ToUpper()</span></div>
    </div>
</div><!--end topBody-->